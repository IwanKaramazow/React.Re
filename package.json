{
  "name": "ReasonExampleProject",
  "private": true,
  "version": "1.0.0",
  "description": "Example project using Reason syntax",
  "license": "BSD",
  "dependencies": {
    "CommonML": "git://github.com/jordwalke/CommonML.git",
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "scripts": {
    "build": "node ./node_modules/CommonML/build.js",
    "start": "./_build_byte/ReasonExampleProject/reasonExampleProject.out"
  },
  "CommonML": {
    "exports": [
      "MyProjectMod"
    ],
    "extensions": [
      {
        "interface": ".rei",
        "implementation": ".re"
      }
    ],
    "preprocessor": "refmt",
    "compileFlags": [
      "-w",
      "-30",
      "-w",
      "-40",
      "-bin-annot"
    ],
    "findlibPackages": [
      {
        "dependency": "js_of_ocaml"
      }
    ],
    "linkFlags": [],
    "jsPlaceBuildArtifactsIn": "webAppRoot",
    "docFlags": []
  },
  "devDependencies": {
    "sane": "^1.3.4",
    "webpack": "^2.1.0-beta.13",
    "webpack-dev-server": "^2.1.0-beta.0"
  }
}
